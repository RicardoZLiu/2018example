#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_babylon@7.0.0-beta.27@babylon/bin/babylon.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_babylon@7.0.0-beta.27@babylon/bin/babylon.js" "$@"
  ret=$?
fi
exit $ret
